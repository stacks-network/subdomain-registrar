    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^2.10.2",
    "eslint-plugin-import": "^1.8.1",
    "flow-bin": "^0.49.1",
    "nock": "^9.1.6",
    "nyc": "^11.4.1",
    "tape": "^4.6.3",
    "tape-istanbul": "^1.2.0"
  },
  "bin": {
    "blockstack-subdomain-registrar": "./lib/index.js"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
